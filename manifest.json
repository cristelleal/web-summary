{
  "manifest_version": 3,
  "name": "Web Summary",
  "author": "Cristelle A.",
  "version": "1.0.0",
  "description": "Une extension Chrome pour r√©sumer le contenu des pages web, en utilisant la puissance de Cohere AI.",
  "permissions": ["activeTab", "storage", "scripting", "identity"],
  "host_permissions": [
    "https://*.supabase.co/*",
    "https://*.googleapis.com/*",
    "https://*.cohere.ai/*",
    "https://*.cohere.com/*"
  ],
  "oauth2": {
    "client_id": "491056150864-jmoekn7bcjho3kohac0j74v3pcdjb51p.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "key": ""
}
